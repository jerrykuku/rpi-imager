<?xml version="1.0" encoding="UTF-8"?>
<!-- MIME type for ZimaOS USB Creator manifest files.
     Extension and MIME type defined in src/config.h (MANIFEST_EXTENSION, MANIFEST_MIME_TYPE) -->
<mime-info xmlns="http://www.freedesktop.org/standards/shared-mime-info">
  <mime-type type="application/vnd.icewhaletech.zimaos-usb-creator-manifest+json">
    <comment>ZimaOS USB Creator Manifest</comment>
    <comment xml:lang="zh_CN">ZimaOS USB 创建工具清单</comment>
    <glob pattern="*.zimaos-usb-creator-manifest"/>
    <sub-class-of type="application/json"/>
    <icon name="rpi-imager"/>
  </mime-type>
</mime-info>
